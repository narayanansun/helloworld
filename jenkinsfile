pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: 'main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/DevOlabodeM/pytest-intro-vs-M']]])
            }
        }
        stage('Build') {
            steps {
                sh 'python3 helloworld.py'
            }
        }
        stage('Test') {
            steps {
                sh 'python3 -m helloworld.py'
            }
        }
    }
}

